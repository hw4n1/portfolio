{
  "name": "juan-munoz-portfolio",
  "version": "1.0.0",
  "description": "Cybersecurity portfolio website for Juan Esteban Muñoz Díaz - Security Engineer",
  "author": "Juan Esteban Muñoz Díaz <hola@naing.co>",
  "repository": {
    "type": "git",
    "url": "https://github.com/hw4n1/portfolio.git"
  },
  "homepage": "https://naing.co",
  "private": true,
  "type": "module",
  "keywords": [
    "portfolio",
    "cybersecurity",
    "security-engineer",
    "react",
    "typescript",
    "vite"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "@emotion/react": "^11.13.3",
    "framer-motion": "^11.5.4",
    "lucide-react": "0.522.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.18",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.50.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "prettier": "^3.2.5",
    "typescript": "^5.5.4",
    "vite": "^5.2.0",
    "tailwindcss": "3.4.17",
    "autoprefixer": "latest",
    "postcss": "latest"
  }
